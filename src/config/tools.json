[
  {
    "name": "swap",
    "description": "Swap tokens from one token to another",
    "parameters": {
      "type": "object",
      "properties": {
        "toToken": {
          "type": "string", 
          "description": "The token to swap to"
        },
        "fromToken": {
          "type": "string",
          "description": "The token to swap from"
        },
        "validatorSalt": {
          "type": "string",
          "description": "The validator salt"
        },
        "amount": {
          "type": "string",
          "description": "The amount of tokens to swap"
        },
        "accountAddress": {
          "type": "string",
          "description": "The account address"
        }
      },
      "required": ["toToken", "fromToken", "validatorSalt", "amount", "accountAddress"]
    }
  },
  {
    "name": "send",
    "description": "Send tokens to an address from the account address",
    "parameters": {
      "type": "object",
      "properties": {
        "toAddress": {
          "type": "string",
          "description": "The address to send tokens to"
        },
        "amount": {
          "type": "string",
          "description": "The amount of tokens to send"
        },
        "token": {
          "type": "string",
          "description": "The token to send"
        },
        "validatorSalt": {
          "type": "string",
          "description": "The validator salt"
        }
      },
      "required": ["toAddress", "amount", "token", "validatorSalt"]
    }
  }
] 